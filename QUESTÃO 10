#include <stdio.h>

int main(void) {
  
  char x1[4] = {"4092"};
  int x2[4] = {4,0,9,2};
  float x3[4] = {4,0,9,2};
  double x4[4] = {4,0,9,2};
  int i;

  for (i=0; i<4;i++){
    printf("char: x + %d = %p\n", i+1, (x1+i));}
    
  printf("\n");
  
   for (i=0; i<4;i++){
    printf("int: x + %d = %p\n", i+1, (x2+i));}
     
  printf("\n");
  
   for (i=0; i<4;i++){
    printf("float: x + %d = %p\n", i+1, (x3+i));}
  
  printf("\n");

   for (i=0; i<4;i++){
    printf("double: x + %d = %p\n", i+1, (x4+i));} 
     
  return 0;
}
